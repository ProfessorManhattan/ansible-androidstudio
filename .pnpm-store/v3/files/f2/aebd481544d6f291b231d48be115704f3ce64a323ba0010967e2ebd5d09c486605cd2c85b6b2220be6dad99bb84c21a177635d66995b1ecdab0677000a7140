    /**
     * Commit the current token.
     */
    endToken(type, pos, option1, option2) {
        const { tokenStart } = this;
        const end = this.codePointIterator[pos];
        const range = [tokenStart.offset, end.offset];
        const loc = {
            start: {
                line: tokenStart.line,
                column: tokenStart.column,
            },
            end: {
                line: end.line,
                column: end.column,
            },
        };
        if (type === "Block") {
            this.token = {
                type,
                value: this.text.slice(tokenStart.offset + 1, end.offset),
                range,
                loc,
            };
        }
        else {
            let token;
            const value = type === "Punctuator"
                ? String.fromCodePoint(option1)
                : this.text.slice(tokenStart.offset, end.offset);
            if (type === "BasicString" ||
                type === "LiteralString" ||
                type === "MultiLineBasicString" ||
                type === "MultiLineLiteralString") {
                token = {
                    type,
                    value,
                    string: String.fromCodePoint(...option1),
                    range,
                    loc,
                };
            }
            else if (type === "Integer") {
                const text = String.fromCodePoint(...option1);
                token = {
                    type,
                    value,
                    number: parseInt(text, option2),
                    bigint: HAS_BIGINT
                        ? BigInt(RADIX_PREFIXES[option2] + text)
                        : null,
                    range,
                    loc,
                };
            }
            else if (type === "Float") {
                token = {
                    type,
                    value,
                    number: option1,
                    range,
                    loc,
                };
            }
            else if (type === "Boolean") {
                token = {
                    type,
                    value,
                    boolean: option1,
                    range,
                    loc,
                };
            }
            else {
                token = {
                    type,
                    value,
                    range,
                    loc,
                };
            }
            this.token = token;
        }
    }
    DATA(cp) {
        while (code_point_1.isWhitespace(cp) || code_point_1.isEOL(cp)) {
            cp = this.nextCode();
        }
        if (cp === code_point_1.HASH) {
            this.startToken();
            return "COMMENT";
        }
        if (cp === code_point_1.QUOTATION_MARK) {
            this.startToken();
            return "BASIC_STRING";
        }
        if (cp === code_point_1.SINGLE_QUOTE) {
            this.startToken();
            return "LITERAL_STRING";
        }
        if (cp === code_point_1.DOT || // .
            cp === code_point_1.EQUALS_SIGN || // =
            cp === code_point_1.LEFT_BRACKET || // [
            cp === code_point_1.RIGHT_BRACKET || // ]
            cp === code_point_1.LEFT_BRACE || // {
            cp === code_point_1.RIGHT_BRACE || // }
            cp === code_point_1.COMMA // ,
        ) {
            this.punctuatorToken(cp);
            return "DATA";
        }
        if (this.valuesEnabled) {
            if (cp === code_point_1.DASH || cp === code_point_1.PLUS_SIGN) {
                this.startToken();
                return "SIGN";
            }
            if (cp === code_point_1.LATIN_SMALL_N || cp === code_point_1.LATIN_SMALL_I) {
                this.startToken();
                return this.back("NAN_OR_INF");
            }
            if (code_point_1.isDigit(cp)) {
                this.startToken();
                return this.back("NUMBER");
            }
            if (cp === code_point_1.LATIN_SMALL_T || cp === code_point_1.LATIN_SMALL_F) {
                this.startToken();
                return this.back("BOOLEAN");
            }
        }
        else {
            if (isBare(cp)) {
                this.startToken();
                return "BARE";
            }
        }
        if (cp === code_point_1.EOF) {
            // end
            return "DATA";
        }
        return this.reportParseError("unexpected-char");
    }
    COMMENT(cp) {
        while (!code_point_1.isEOL(cp) && cp !== code_point_1.EOF) {
            if (isControlOtherThanTab(cp)) {
                return this.reportParseErrorControlChar();
            }
            cp = this.nextCode();
        }
        this.endToken("Block", "start");
        return "DATA";
    }
    BARE(cp) {
        while (isBare(cp)) {
            cp = this.nextCode();
        }
        this.endToken("Bare", "start");
        return this.back("DATA");
    }
    BASIC_STRING(cp) {
        if (cp === code_point_1.QUOTATION_MARK) {
            cp = this.nextCode();
            if (cp === code_point_1.QUOTATION_MARK) {
                return "MULTI_LINE_BASIC_STRING";
            }
            this.endToken("BasicString", "start", []);
            return this.back("DATA");
        }
        const codePoints = [];
        while (cp !== code_point_1.QUOTATION_MARK && cp !== code_point_1.EOF && cp !== code_point_1.LINE_FEED) {
            if (isControlOtherThanTab(cp)) {
                return this.reportParseErrorControlChar();
            }
            if (cp === code_point_1.BACKSLASH) {
                cp = this.nextCode();
                const ecp = ESCAPES[cp];
                if (ecp) {
                    codePoints.push(ecp);
                    cp = this.nextCode();
                    continue;
                }
                else if (cp === code_point_1.LATIN_SMALL_U) {
