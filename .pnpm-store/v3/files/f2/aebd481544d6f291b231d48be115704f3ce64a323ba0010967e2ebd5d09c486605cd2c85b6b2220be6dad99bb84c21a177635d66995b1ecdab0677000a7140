                }
                else if (cp === code_point_1.LATIN_CAPITAL_U) {
                    const code = this.parseUnicode(8);
                    codePoints.push(code);
                    cp = this.nextCode();
                    continue;
                }
                return this.reportParseError("invalid-char-in-escape-sequence");
            }
            codePoints.push(cp);
            cp = this.nextCode();
        }
        if (cp !== code_point_1.QUOTATION_MARK) {
            return this.reportParseError("unterminated-string");
        }
        this.endToken("BasicString", "end", codePoints);
        return "DATA";
    }
    MULTI_LINE_BASIC_STRING(cp) {
        const codePoints = [];
        if (cp === code_point_1.LINE_FEED) {
            // A newline immediately following the opening delimiter will be trimmed.
            cp = this.nextCode();
        }
        while (cp !== code_point_1.EOF) {
            if (cp !== code_point_1.LINE_FEED && isControlOtherThanTab(cp)) {
                return this.reportParseErrorControlChar();
            }
            if (cp === code_point_1.QUOTATION_MARK) {
                const nextPoints = this.codePointIterator.subCodePoints();
                if (nextPoints.next() === code_point_1.QUOTATION_MARK &&
                    nextPoints.next() === code_point_1.QUOTATION_MARK) {
                    if (nextPoints.next() === code_point_1.QUOTATION_MARK) {
                        codePoints.push(code_point_1.QUOTATION_MARK);
                        if (nextPoints.next() === code_point_1.QUOTATION_MARK) {
                            codePoints.push(code_point_1.QUOTATION_MARK);
                            if (nextPoints.next() === code_point_1.QUOTATION_MARK) {
                                return this.reportParseError("invalid-three-quotes");
                            }
                        }
                    }
                    this.skip(nextPoints.count - 1);
                    // end
                    this.endToken("MultiLineBasicString", "end", codePoints);
                    return "DATA";
                }
            }
            if (cp === code_point_1.BACKSLASH) {
                cp = this.nextCode();
                const ecp = ESCAPES[cp];
                if (ecp) {
                    codePoints.push(ecp);
                    cp = this.nextCode();
                    continue;
                }
                else if (cp === code_point_1.LATIN_SMALL_U) {
                    const code = this.parseUnicode(4);
                    codePoints.push(code);
                    cp = this.nextCode();
                    continue;
                }
                else if (cp === code_point_1.LATIN_CAPITAL_U) {
                    const code = this.parseUnicode(8);
                    codePoints.push(code);
                    cp = this.nextCode();
                    continue;
                }
                else if (cp === code_point_1.LINE_FEED) {
                    cp = this.nextCode();
                    while (code_point_1.isWhitespace(cp) || cp === code_point_1.LINE_FEED) {
                        cp = this.nextCode();
                    }
                    continue;
                }
                else if (code_point_1.isWhitespace(cp)) {
                    let valid = true;
                    for (const nextCp of this.codePointIterator.iterateSubCodePoints()) {
                        if (nextCp === code_point_1.LINE_FEED) {
                            break;
                        }
                        if (!code_point_1.isWhitespace(nextCp)) {
                            valid = false;
                            break;
                        }
                    }
                    if (valid) {
                        cp = this.nextCode();
                        while (code_point_1.isWhitespace(cp) || cp === code_point_1.LINE_FEED) {
                            cp = this.nextCode();
                        }
                        continue;
                    }
                }
                return this.reportParseError("invalid-char-in-escape-sequence");
            }
            codePoints.push(cp);
            cp = this.nextCode();
        }
        return this.reportParseError("unterminated-string");
    }
    LITERAL_STRING(cp) {
        if (cp === code_point_1.SINGLE_QUOTE) {
            cp = this.nextCode();
            if (cp === code_point_1.SINGLE_QUOTE) {
                return "MULTI_LINE_LITERAL_STRING";
