                const nextPoints = this.codePointIterator.subCodePoints();
                if (nextPoints.next() === code_point_1.SINGLE_QUOTE &&
                    nextPoints.next() === code_point_1.SINGLE_QUOTE) {
                    if (nextPoints.next() === code_point_1.SINGLE_QUOTE) {
                        codePoints.push(code_point_1.SINGLE_QUOTE);
                        if (nextPoints.next() === code_point_1.SINGLE_QUOTE) {
                            codePoints.push(code_point_1.SINGLE_QUOTE);
                            if (nextPoints.next() === code_point_1.SINGLE_QUOTE) {
                                return this.reportParseError("invalid-three-quotes");
                            }
