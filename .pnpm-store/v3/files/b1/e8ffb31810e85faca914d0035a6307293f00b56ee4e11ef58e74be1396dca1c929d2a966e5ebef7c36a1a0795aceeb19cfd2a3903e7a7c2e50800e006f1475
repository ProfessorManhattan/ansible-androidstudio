export declare const EOF = -1;
export declare const NULL = 0;
export declare const BACKSPACE = 8;
export declare const TABULATION = 9;
export declare const LINE_FEED = 10;
export declare const FORM_FEED = 12;
export declare const CARRIAGE_RETURN = 13;
export declare const US = 31;
export declare const SPACE = 32;
export declare const QUOTATION_MARK = 34;
export declare const HASH = 35;
export declare const SINGLE_QUOTE = 39;
export declare const PLUS_SIGN = 43;
export declare const COMMA = 44;
export declare const DASH = 45;
export declare const DOT = 46;
export declare const DIGIT_0 = 48;
export declare const DIGIT_1 = 49;
export declare const DIGIT_2 = 50;
export declare const DIGIT_3 = 51;
export declare const DIGIT_7 = 55;
export declare const DIGIT_9 = 57;
export declare const COLON = 58;
export declare const EQUALS_SIGN = 61;
export declare const LATIN_CAPITAL_A = 65;
export declare const LATIN_CAPITAL_E = 69;
export declare const LATIN_CAPITAL_F = 70;
export declare const LATIN_CAPITAL_T = 84;
export declare const LATIN_CAPITAL_U = 85;
export declare const LATIN_CAPITAL_Z = 90;
export declare const LEFT_BRACKET = 91;
export declare const BACKSLASH = 92;
export declare const RIGHT_BRACKET = 93;
export declare const UNDERSCORE = 95;
export declare const LATIN_SMALL_A = 97;
export declare const LATIN_SMALL_B = 98;
export declare const LATIN_SMALL_E = 101;
export declare const LATIN_SMALL_F = 102;
export declare const LATIN_SMALL_I = 105;
export declare const LATIN_SMALL_L = 108;
export declare const LATIN_SMALL_N = 110;
export declare const LATIN_SMALL_O = 111;
export declare const LATIN_SMALL_R = 114;
export declare const LATIN_SMALL_S = 115;
export declare const LATIN_SMALL_T = 116;
export declare const LATIN_SMALL_U = 117;
export declare const LATIN_SMALL_X = 120;
export declare const LATIN_SMALL_Z = 122;
export declare const LEFT_BRACE = 123;
export declare const RIGHT_BRACE = 125;
export declare const DELETE = 127;
/**
 * Check whether the code point is a control character.
 */
export declare function isControl(cp: number): boolean;
/**
 * Check whether the code point is a whitespace.
 */
export declare function isWhitespace(cp: number): boolean;
/**
 * Check whether the code point is a end of line.
 */
export declare function isEOL(cp: number): boolean;
/**
 * Check whether the code point is a letter character.
 */
export declare function isLetter(cp: number): boolean;
/**
 * Check whether the code point is a digit character.
 */
export declare function isDigit(cp: number): boolean;
/**
 * Check whether the code point is a hex digit character.
 */
export declare function isHexDig(cp: number): boolean;
/**
 * Check whether the code point is a octal digit character.
 */
export declare function isOctalDig(cp: number): boolean;
/**
 * Check whether the code point is a high-surrogate code point.
 */
export declare function isHighSurrogate(cp: number): boolean;
/**
 * Check whether the code point is a low-surrogate code point.
 */
export declare function isLowSurrogate(cp: number): boolean;
/**
 * Check whether the code point is valid code point.
 *
 * see
 * - https://unicode.org/glossary/#unicode_scalar_value
 * - https://toml.io/en/v1.0.0#string
 */
export declare function isUnicodeScalarValue(cp: number): boolean;
