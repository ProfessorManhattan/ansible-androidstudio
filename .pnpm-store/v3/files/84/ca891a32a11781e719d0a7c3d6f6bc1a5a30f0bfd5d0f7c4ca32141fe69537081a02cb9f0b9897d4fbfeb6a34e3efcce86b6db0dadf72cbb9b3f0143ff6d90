/**
 * Expose a single rule called "config", which will be accessed in the user's eslint config files
 * via "tslint/config"
 */
export declare const rules: {
    config: import("@typescript-eslint/experimental-utils/dist/ts-eslint/Rule").RuleModule<"failure", import("./rules/config").Options, import("@typescript-eslint/experimental-utils/dist/ts-eslint/Rule").RuleListener>;
};
