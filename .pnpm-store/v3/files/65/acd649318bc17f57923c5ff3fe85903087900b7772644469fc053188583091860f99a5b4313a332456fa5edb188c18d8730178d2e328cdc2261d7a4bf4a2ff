import { RuleSeverity } from 'tslint';
export declare type RawRulesConfig = Record<string, null | undefined | boolean | unknown[] | {
    severity?: RuleSeverity | 'warn' | 'none' | 'default';
    options?: unknown;
}>;
export declare type MessageIds = 'failure';
export declare type Options = [
    {
        rules?: RawRulesConfig;
        rulesDirectory?: string[];
        lintFile?: string;
    }
];
declare const _default: import("@typescript-eslint/experimental-utils/dist/ts-eslint/Rule").RuleModule<"failure", Options, import("@typescript-eslint/experimental-utils/dist/ts-eslint/Rule").RuleListener>;
export default _default;
