{
  "name": "@installdoc/ansible-androidstudio",
  "version": "1.1.1",
  "description": "Installs Android Studio and sets up Android SDKs on nearly any OS",
  "license": "MIT",
  "author": "Brian Zalewski <brian@megabyte.space> (https://megabyte.space)",
  "homepage": "https://megabyte.space",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ProfessorManhattan/ansible-androidstudio.git"
  },
  "bugs": {
    "email": "help@megabyte.space",
    "url": "https://gitlab.com/megabyte-labs/ansible-roles/androidstudio/-/issues"
  },
  "type": "module",
  "scripts": {
    "clean": "task common:clean",
    "commit": "task common:commit",
    "info": "task --list",
    "prepare": "task common:husky",
    "prepare-release": "task common:prepare-release",
    "requirements": "task common:requirements",
    "reset": "task common:reset",
    "reset-force": "task common:reset-force",
    "update": "task common:update"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@megabytelabs/prettier-config": "^2.0.0",
    "@prettier/plugin-xml": "^1.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-jsonc": "^1.6.0",
    "eslint-plugin-yml": "0.10.1",
    "prettier": "^2.4.0",
    "prettier-plugin-sh": "^0.7.1",
    "prettier-plugin-toml": "^0.3.1"
  },
  "keywords": [
    "adb",
    "android",
    "androidsdk",
    "androidstudio",
    "ansible",
    "apps",
    "archlinux",
    "awesome",
    "centos",
    "debian",
    "desktop",
    "development",
    "doctor",
    "fedora",
    "google",
    "ide",
    "install",
    "installdoc",
    "installdoctor",
    "java",
    "macos",
    "megabytelabs",
    "molecule",
    "platformtools",
    "professormanhattan",
    "role",
    "snap",
    "ubuntu",
    "windows"
  ],
  "engines": {
    "node": ">=10"
  },
  "blueprint": {
    "description": "Installs Android Studio and sets up Android SDKs on nearly any OS",
    "gitlab_path": "megabyte-labs/ansible-roles/androidstudio",
    "name": "Android Studio",
    "overview": "[Android Studio](https://developer.android.com/studio) is the official integrated development environment for Google's Android operating system, built on JetBrains' IntelliJ IDEA software and designed specifically for Android development.",
    "repository": {
      "github": "https://gitlab.com/ProfessorManhattan/ansible-androidstudio",
      "gitlab": "https://gitlab.com/megabyte-labs/ansible-roles/androidstudio"
    },
    "slug": "androidstudio",
    "title": "Android Studio - An IDE for Android Development"
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/megabytelabs"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/ProfessorManhattan"
    }
  ],
  "lint-staged": {
    "**/*": [
      "blocklint --wordlist blacklist,slave,whitelist",
      "check-merge-conflict",
      "check-added-large-files",
      "check-symlinks",
      "destroyed-symlinks"
    ],
    "**/*.(css|html|json|less|mjml|php|scss|sh|ts)": [
      "prettier --write"
    ],
    "**/*.(jpg|png)": [
      "task image:compress --"
    ],
    "**/*.js": [
      "npx standard --fix",
      "prettier --write"
    ],
    "**/!(package).json": [
      "check-json",
      "task fix:json --",
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ],
    "**/*.(sh|sh.j2)": [
      "shellcheck",
      "prettier --write"
    ],
    "**/*.toml": [
      "check-toml",
      "prettier --write"
    ],
    "**/*.xml": [
      "check-xml",
      "prettier --write"
    ],
    "**/*.yml": [
      "check-yaml",
      "yamllint -s",
      "task fix:yaml --",
      "prettier --write"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ],
    "package.json": [
      "check-json",
      "task fix:json --",
      "prettier-package-json --write"
    ],
    "requirements.txt": [
      "requirements-txt-fixer"
    ],
    "**/*.py": [
      "black",
      "flake8"
    ],
    "**/*vault.yml": [
      "task ansible:vault:lint:file --"
    ]
  },
  "prettier": "@megabytelabs/prettier-config",
  "standard-version": {
    "scripts": {
      "prerelease": "git add --all"
    }
  }
}
