{
  "name": "@megabytelabs/ansible-",
  "version": "1.0.4",
  "description": "",
  "license": "MIT",
  "author": "Brian Zalewski <brian@megabyte.space> (https://megabyte.space)",
  "homepage": "https://megabyte.space",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/megabyte-labs/ansible-roles/androidstudio.git"
  },
  "bugs": {
    "email": "help@megabyte.space",
    "url": "https://gitlab.com/megabyte-labs/ansible-roles/androidstudio/-/issues"
  },
  "type": "module",
  "scripts": {
    "commit": "git-cz",
    "postinstall": "if [ -d .git ]; then husky install; fi && if [ -f .husky/pre-commit ]; then chmod 755 .husky/pre-commit; fi",
    "prepare-release": "task prepare-release",
    "version": "standard-version --no-verify"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@megabytelabs/prettier-config": "^1.0.0",
    "@megabytelabs/prettier-config-ansible": "^1.1.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "inquirer": "^6.5.2",
    "lint-staged": "^11.0.0",
    "prettier-plugin-sh": "^0.6.1",
    "signale": "^1.4.0"
  },
  "keywords": [
    "androidstudio",
    "ansible",
    "archlinux",
    "centos",
    "debian",
    "doctor",
    "fedora",
    "install",
    "installdoctor",
    "macos",
    "megabytelabs",
    "molecule",
    "professormanhattan",
    "role",
    "ubuntu",
    "windows"
  ],
  "engines": {
    "node": ">=10"
  },
  "blueprint": {
    "ansible_galaxy_project_id": "",
    "compatibility_date": "",
    "compatibility_matrix": [],
    "name": "Android Studio",
    "overview": "[Android Studio](https://developer.android.com/studio) is the official integrated development environment for Google's Android operating system, built on JetBrains' IntelliJ IDEA software and designed specifically for Android development.",
    "repo": "https://gitlab.com/megabyte-labs/ansible-roles/androidstudio",
    "role_descriptions": [],
    "title": "Android Studio - An IDE for Android Development",
    "requirements": [
      [
        "snapd",
        "Ensures Snap is installed and properly configured on Linux systems"
      ],
      [
        "homebrew",
        "Installs Homebrew (a package management system) on macOS and Linux systems"
      ]
    ]
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/megabytelabs"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/ProfessorManhattan"
    }
  ],
  "lint-staged": {
    "**/*": [
      "check-merge-conflict",
      "check-added-large-files",
      "check-symlinks"
    ],
    "**/*.(css|html|json|less|md|mjml|php|scss|sh|ts)": [
      "prettier --write"
    ],
    "**/*.js": [
      "npx standard --fix",
      "prettier --write"
    ],
    "**/*.(sh|sh.j2)": [
      "shellcheck"
    ],
    "**/*vault.yml": [
      "bash .common/scripts/ansible-vault.sh"
    ],
    "**/*.xml": [
      "check-xml",
      "prettier --write"
    ],
    "**/*.yml": [
      "check-yaml",
      "yamllint -s",
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ],
    "requirements.txt": [
      "requirements-txt-fixer"
    ],
    "./**/*.py": [
      "black",
      "flake8"
    ]
  },
  "prettier": "@megabytelabs/prettier-config-ansible",
  "standard-version": {
    "scripts": {
      "prerelease": "git add --all"
    }
  }
}
