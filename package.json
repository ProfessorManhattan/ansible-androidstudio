{
  "name": "@installdoc/androidstudio",
  "version": "1.0.4",
  "description": "Installs Android Studio and sets up Android SDKs on nearly any OS",
  "license": "MIT",
  "author": "Brian Zalewski <brian@megabyte.space> (https://megabyte.space)",
  "homepage": "https://megabyte.space",
  "repository": {
    "type": "git",
    "url": "git+[[ package.json .blueprint.repo - See CONTRIBUTING.md ]].git"
  },
  "bugs": {
    "email": "help@megabyte.space",
    "url": "[[ package.json .blueprint.repo - See CONTRIBUTING.md ]][object Object]"
  },
  "type": "module",
  "scripts": {
    "clean": "task common:clean",
    "commit": "task common:commit",
    "info": "task --list",
    "prepare": "bash .common/scripts/npm-prepare.sh",
    "prepare-release": "task common:prepare-release",
    "requirements": "task common:requirements",
    "reset": "task common:reset",
    "reset-force": "task common:reset-force",
    "update": "task common:update",
    "version": "standard-version --no-verify"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@megabytelabs/prettier-config": "^2.0.0",
    "@megabytelabs/prettier-config-ansible": "^1.1.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "inquirer": "^6.5.2",
    "lint-staged": "^11.0.0",
    "prettier-plugin-sh": "^0.6.1",
    "remark-preset-lint-recommended": "^5.0.0",
    "signale": "^1.4.0"
  },
  "keywords": [
    "androidstudio",
    "ansible",
    "archlinux",
    "centos",
    "debian",
    "doctor",
    "fedora",
    "install",
    "installdoc",
    "installdoctor",
    "macos",
    "megabytelabs",
    "molecule",
    "professormanhattan",
    "role",
    "ubuntu",
    "windows"
  ],
  "engines": {
    "node": ">=10"
  },
  "blueprint": {
    "ansible_galaxy_project_id": "",
    "compatibility_matrix": [],
    "description": "Installs Android Studio and sets up Android SDKs on nearly any OS",
    "name": "Android Studio",
    "overview": "[Android Studio](https://developer.android.com/studio) is the official integrated development environment for Google's Android operating system, built on JetBrains' IntelliJ IDEA software and designed specifically for Android development.",
    "repository": {
      "github": "https://gitlab.com/ProfessorManhattan/ansible-androidstudio",
      "gitlab": "https://gitlab.com/megabyte-labs/ansible-roles/androidstudio"
    },
    "requirements": [
      [
        "snapd",
        "Ensures Snap is installed and properly configured on Linux systems"
      ],
      [
        "homebrew",
        "Installs Homebrew (a package management system) on macOS and Linux systems"
      ]
    ],
    "role_descriptions": [],
    "slug": "androidstudio",
    "title": "Android Studio - An IDE for Android Development"
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/megabytelabs"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/ProfessorManhattan"
    }
  ],
  "lint-staged": {
    "**/*": [
      "blocklint --wordlist blacklist,slave,whitelist",
      "check-merge-conflict",
      "check-added-large-files",
      "check-symlinks",
      "destroyed-symlinks"
    ],
    "**/*.(css|html|json|less|mjml|php|scss|sh|ts)": [
      "prettier --write"
    ],
    "**/*.(jpg|png)": [
      "task image:compress --"
    ],
    "**/*.js": [
      "npx standard --fix",
      "prettier --write"
    ],
    "**/!(package).json": [
      "check-json",
      "task fix:json --",
      "prettier --write"
    ],
    "**/*.md": [
      "remark",
      "prettier --write"
    ],
    "**/*.(sh|sh.j2)": [
      "shellcheck",
      "prettier --write"
    ],
    "**/*.toml": [
      "check-toml"
    ],
    "**/*.xml": [
      "check-xml",
      "prettier --write"
    ],
    "**/*.yml": [
      "check-yaml",
      "yamllint -s",
      "task fix:yml-dashes --",
      "prettier --write"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ],
    "package.json": [
      "check-json",
      "task fix:json --",
      "prettier-package-json --write"
    ],
    "requirements.txt": [
      "requirements-txt-fixer"
    ],
    "**/*.py": [
      "black",
      "flake8"
    ],
    "**/*vault.yml": [
      "bash .common/scripts/ansible/vault.sh"
    ]
  },
  "prettier": "@megabytelabs/prettier-config",
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "standard-version": {
    "scripts": {
      "prerelease": "git add --all"
    }
  }
}
