{
  "name": "@installdoc/ansible-androidstudio",
  "version": "1.1.1",
  "description": "Installs Android Studio and sets up Android SDKs on nearly any OS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfessorManhattan/ansible-androidstudio.git"
  },
  "author": "Brian Zalewski <brian@megabyte.space> (https://megabyte.space)",
  "homepage": "https://megabyte.space",
  "bugs": {
    "email": "help@megabyte.space",
    "url": "https://gitlab.com/megabyte-labs/ansible-roles/androidstudio/-/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "info": "task --list",
    "prepare": "task common:husky",
    "test": "git reset --hard HEAD && git clean -fxd && cd .commonshared && git pull origin master && cd .. && cp -rT .commonshared/common/.config .config && task start"
  },
  "keywords": [
    "adb",
    "android",
    "androidsdk",
    "androidstudio",
    "ansible",
    "apps",
    "archlinux",
    "awesome",
    "centos",
    "debian",
    "desktop",
    "development",
    "doctor",
    "fedora",
    "google",
    "ide",
    "install",
    "installdoc",
    "installdoctor",
    "java",
    "macos",
    "megabytelabs",
    "molecule",
    "platformtools",
    "professormanhattan",
    "role",
    "snap",
    "ubuntu",
    "windows"
  ],
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@mblabs/eslint-config": "^1.0.0",
    "@mblabs/prettier-config": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "handlebars-helpers": "^0.10.0"
  },
  "blueprint": {
    "description": "Installs Android Studio and sets up Android SDKs on nearly any OS",
    "name": "Android Studio",
    "group": "ansible",
    "overview": "[Android Studio](https://developer.android.com/studio) is the official integrated development environment for Google's Android operating system, built on JetBrains' IntelliJ IDEA software and designed specifically for Android development. This role installs Android Studio on nearly any operating system and also ensures a configurable list of command-line tools and SDKs are installed and seamlessly integrated with the system (i.e. the role adds items to the `PATH`).",
    "repository": {
      "github": "https://github.com/ProfessorManhattan/ansible-androidstudio",
      "gitlab": "https://gitlab.com/megabyte-labs/ansible-roles/androidstudio"
    },
    "subgroup": "role",
    "slug": "androidstudio",
    "title": "Android Studio - An IDE for Android Development"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/megabytelabs"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/ProfessorManhattan"
    }
  ],
  "lint-staged": {
    "**/*": [
      "blocklint --wordlist blacklist,slave,whitelist",
      "check-merge-conflict",
      "check-added-large-files",
      "check-symlinks",
      "destroyed-symlinks"
    ],
    "**/*.(css|html|less|md|mjml|php|scss|sh|xml)": [
      "task fix:prettier --"
    ],
    "**/*.json": [
      "task fix:json --"
    ],
    "**/*.(js|jsx|ts|tsx)": [
      "task fix:js --",
      "task lint:js --"
    ],
    "**/*.sh": [
      "task fix:shell --",
      "task lint:shell --"
    ],
    "**/*.toml": [
      "task lint:toml --"
    ],
    "**/*.xml": [
      "task fix:xml --",
      "task lint:xml --"
    ],
    "**/*.yml": [
      "task fix:yaml --",
      "task lint:yaml --"
    ],
    "**/*.(jpg|png)": [
      "task image:compress --"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ]
  },
  "prettier": "@mblabs/prettier-config",
  "standard-version": {
    "scripts": {
      "prerelease": "git add --all"
    }
  }
}
